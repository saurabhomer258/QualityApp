<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.v7.widget.CardView
        android:id="@+id/cardView1"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView3"
        tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/Inside"
                android:layout_width="150dp"
                android:layout_height="40dp"
                android:text="INSIDE"
                tools:layout_editor_absoluteX="1dp"
                tools:layout_editor_absoluteY="184dp" />
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="73dp">

        <TextView
            android:id="@+id/info_text1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="OUTSIDE" />


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="150dp"
        android:layout_height="40dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="73dp">

        <TextView
            android:id="@+id/getup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="GETUP" />

    </android.support.v7.widget.CardView>


    <android.support.v7.widget.CardView
    android:id="@+id/cardView4"
    android:layout_width="150dp"
    android:layout_height="40dp"
    android:layout_marginStart="284dp"
        android:layout_marginTop="76dp"
        app:layout_constraintStart_toStartOf="@+id/cardView1"
        app:layout_constraintTop_toBottomOf="@+id/cardView3"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/meamntreport"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="MEASERMENT REPORT" />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/cardView1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp">

        <TextView
            android:id="@+id/metal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="METAL DEDECTION REPORT" />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="150dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/cardView4"
        android:layout_marginTop="40dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/skutest"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="SKU TEST" />


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView7"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/cartoonaudit"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="CARTOON AUDIT" />


    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/cardView8"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView6"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/inline"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="INLINE FINAL INSPECTION" />

    </android.support.v7.widget.CardView>

</android.support.constraint.ConstraintLayout>